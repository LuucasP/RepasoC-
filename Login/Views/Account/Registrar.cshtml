@model p.ViewModels.Registrar


<form asp-action="Registrar" method="post">

    <div asp-asp-validation-summary="All"></div>

    <div class="form-group">
    <label>Usuario</label>
    <input type="text" asp-for="Usuario" class="form-control">

</div>

<div class="form-group">
    <label>Email</label>
    <input type="text" asp-for="Email" class="form-control">

</div>



<div class="form-group">
    <label>Password</label>
    <input type="password" asp-for="Password" class="form-control">

</div>

<div class="form-group">
    <label>Confirmar Pass</label>
    <input type="password" asp-for="PasswordConfirmacion" class="form-control">

</div>

<button>Enviar</button>

</form>